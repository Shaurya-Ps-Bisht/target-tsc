      "destinationType": "topic",
      "isCompressed": false,
      "responseTemplatePath": "test-data_4.json",
      "correlationIdDisabled": true
    },
    "protocol": "Solace"
  },
  "testTemplateData": {
    "templatePath": "template_4.json",
    "templateParameters": {
      "static": {
        "ActionCode": "DebitClearing_CreditBeneficiary",
        "ActionDesc": null,
        "fromBic": "DEBTORBANKAUXX",
        "toBic": "CREDITRBANKAUXX",
        "fromCountry": "AU",
        "toCountry": "AU",
        "fromBranch": "DEBIT_BRANCH_CODE",
        "toBranch": "CREDIT_BRANCH_CODE",
        "currency": "AUD",
        "amount": "150.75",
        "beneficiaryCountry": "AU",
        "beneficiaryBic": "CREDITRBANKAUXX",
        "beneficiaryBranch": "CREDIT_BRANCH_CODE",
        "beneficiaryName": "Creditor Name",
        "beneficiaryAccountId": "CREDIT_ACCT_ID_456",
        "debitCountry": "AU",
        "debitBic": "DEBTORBANKAUXX",
        "debitBranch": "DEBIT_BRANCH_CODE",
        "debitAccountId": "DEBIT_ACCT_ID_123",
        "debitAccountGroup": null,
        "debitAcctSys": "LegacyGL",
        "debitAcctUse": "Suspense",
        "debitCostCentre": null,
        "creditCountry": "AU",
        "creditBic": "CREDITRBANKAUXX",
        "creditBranch": "CREDIT_BRANCH_CODE",
        "creditAccountId": "CREDIT_ACCT_ID_456",
        "creditAccountGroup": "AUA",
        "creditAcctSys": "CoreBankingSys",
        "creditAcctUse": null,
        "creditCostCentre": null,
        "clearPref": "FAST",
        "clearSubPref": "ICS1",
        "chargeBearer": "SLEV",
        "ClientName": "CoreSvc"
