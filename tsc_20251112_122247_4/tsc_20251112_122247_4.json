{
  "testMetadata": {
    "testName": "CoreSvc Accounting Domestic Transfer Test",
    "testDescription": "End-to-end test for CoreSvc Accounting processing a domestic transfer."
  },
  "messaging": {
    "inputQueue": {
      "destination": "P/A/CoreSvc/AU/PMT/Orchestrator/1/PE/N/ACCOUNTING/CoreSvc/FAST/INWARD/NORM",
      "isCompressed": false
    },
    "reply": {
